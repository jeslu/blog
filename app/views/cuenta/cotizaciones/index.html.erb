<p id="notice"><%= notice %></p>

<h1>Cuenta Cotizaciones</h1>

<table>
  <thead>
    <tr>
      <th>Cuenta</th>
      <th>Producto</th>
      <th>Cantidad</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuenta_cotizaciones.each do |cuenta_cotizacion| %>
      <tr>
        <td><%= cuenta_cotizacion.cuenta_id %></td>
        <td><%= cuenta_cotizacion.producto_id %></td>
        <td><%= cuenta_cotizacion.cantidad %></td>
        <td><%= cuenta_cotizacion.total %></td>
        <td><%= link_to 'Show', cuenta_cotizacion %></td>
        <td><%= link_to 'Edit', edit_cuenta_cotizacion_path(cuenta_cotizacion) %></td>
        <td><%= link_to 'Destroy', cuenta_cotizacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cuenta Cotizacion', new_cuenta_cotizacion_path %>
